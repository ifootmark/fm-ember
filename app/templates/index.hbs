<div class="row">
    <div class="col-md-4">
        <table class="table">
            <thead>
                <tr>
                    <th>Brands List</th>
                </tr>
            </thead>
            <tbody>
                {{#each model as |item|}}
                <tr>
                    <td>
                        {{#link-to 'detail' item}}{{item.brandName}}{{/link-to}}
                    </td>
                </tr>
                {{/each}}
            </tbody>
        </table>
    </div>
    <div class="col-md-8">{{outlet}}</div>
</div>